package com.bilal.LifeApp;

public class Life {
	private String message;

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}
	
	public void init() {
		System.out.println("running init()");
	}
	
	public void destroy() {
		System.out.println("running destroy()");
	}

}
